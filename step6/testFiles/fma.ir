READF $L2
WRITES third
READF $L3
PUSH 
PUSH $L1
PUSH $L2
JSR multiply
POP 
POP 
POP $T1
STOREF $T1 $L5
PUSH 
PUSH $L5
PUSH $L3
JSR add
POP 
POP 
POP $T2
STOREF $T2 $L4
WRITEF $L1
WRITES star
WRITEF $L2
